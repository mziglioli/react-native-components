{
  "name": "@mziglioli/react-native-components",
  "version": "1.6.1",
  "description": "library for react native components",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "react-native-components.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "release-pipeline": "read -p 'PUBLISH_TOKEN: ' PUBLISH_TOKEN && export PUBLISH_TOKEN=$PUBLISH_TOKEN && release-it",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "bootstrap": "yarn"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "url": "git://github.com/mziglioli/react-native-components.git"
  },
  "author": "mziglioli <marceloziglioli@gmail.com> (https://github.com/mziglioli)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mziglioli/react-native-components/issues"
  },
  "homepage": "https://github.com/mziglioli/react-native-components#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/drawer": "^5.12.5",
    "@react-navigation/material-bottom-tabs": "^5.3.15",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@testing-library/jest-native": "^4.0.1",
    "expo": "^40.0.0",
    "expo-asset": "^8.3.2",
    "expo-cli": "^4.0.13",
    "expo-constants": "^11.0.1",
    "expo-file-system": "~11.1.3",
    "expo-font": "^9.2.0",
    "expo-linear-gradient": "~8.2.1",
    "expo-splash-screen": "~0.8.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "^0.18.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-unimodules": "~0.12.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-web": "0.17.1",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/runtime": "^7.9.6",
    "@commitlint/config-conventional": "^11.0.0",
    "@expo/vector-icons": "^12.0.5",
    "@expo/webpack-config": "^0.11.11",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addons": "~5.2.1",
    "@storybook/react": "~5.2.1",
    "@storybook/react-native": "^5.3.25",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "0.62.13",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "8.3.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-native-a11y": "^2.0.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-expo": "^38.0.2",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "release-it": "^14.2.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@expo/vector-icons": "^12.0.5",
    "expo": "^40.0.0",
    "expo-asset": "^8.3.2",
    "expo-constants": "^11.0.1",
    "expo-file-system": "~11.1.3",
    "expo-font": "^9.2.0",
    "expo-splash-screen": "~0.8.1",
    "react": "*",
    "react-native": "*",
    "react-native-paper": "^4.9.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true,
      "skipChecks": true
    },
    "github": {
      "release": true,
      "skipChecks": true,
      "tokenRef": "NPM_TOKEN"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "jest.setup-after-env.ts",
    "jest.config.js"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
